{
    "name": "pict-section-entitymanagement",
    "version": "1.0.1",
    "description": "Dynamic entity management sections leveraging pict-section-forms metatemplates.",
    "main": "source/Pict-Section-EntityManagement.js",
    "scripts": {
        "start": "node source/Pict-Section-EntityManagement.js",
        "test": "npx quack test",
        "tests": "npx quack test -g",
        "coverage": "npx quack coverage",
        "build": "npx quack build"
    },
    "author": "steven velozo <steven@velozo.com>",
    "license": "MIT",
    "devDependencies": {
        "quackage": "^1.0.58"
    },
    "mocha": {
        "diff": true,
        "extension": [
            "js"
        ],
        "package": "./package.json",
        "reporter": "spec",
        "slow": "75",
        "timeout": "5000",
        "ui": "tdd",
        "watch-files": [
            "source/**/*.js",
            "test/**/*.js"
        ],
        "watch-ignore": [
            "lib/vendor"
        ]
    },
    "dependencies": {
        "pict-router": "^1.0.6",
        "pict-section-form": "^1.0.193"
    }
}
